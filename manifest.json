{
  "manifest_version": 3,
  "name": "VibeTab",
  "description": "A minimal new tab with vibey video backgrounds.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [],
  "host_permissions": [],
  "web_accessible_resources": [
    {
      "resources": ["bg.mp4"],
      "matches": ["<all_urls>"]
    }
  ]
}
